package cards;

public class CardsMain {

	public static void main(String[] args) {
		Card best = new Card();
		Card worst = new Card();
		
		best.setNumber("first");
		best.setRank("Ace");
		best.setSuit("Spades");
		best.printCard();
		
		worst.setNumber("second");
		worst.setRank("two");
		worst.setSuit("Club");
		worst.printCard();

	}

}
